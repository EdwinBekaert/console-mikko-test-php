{
  "name": "edwinbekaert/console-mikko-test-php",
  "description": "command-line utility in PHP to determine the dates to pay salaries to the sales department.",
  "type": "project",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Edwin Bekaert",
      "email": "edwin@jumpstart.be",
      "homepage": "https://github.com/EdwinBekaert"
    }
  ],
  "require": {
    "php": "^7.2",
    "c/config": "^4.2",
    "symfony/console": "^4.2",
    "symfony/dependency-injection": "^4.2",
    "symfony/yaml": "^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "symfony/var-dumper": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": ".\\vendor\\bin\\phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
